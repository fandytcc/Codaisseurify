<div class="row">
  <% @artists.each do |artist| %>
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <img src="https://picsum.photos/200/200/?random" alt="random image">
        <div class="caption">
          <h3><%= artist.first_name %></h3>
          <h4><%= artist.last_name %> </h4>
          <p><%= link_to "VIEW", artist, class: "btn btn-default"%></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
